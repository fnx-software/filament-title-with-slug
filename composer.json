{
    "name": "mstfkhazaal/filament-title-with-slug",
    "description": "This is my package filament-title-with-slug",
    "keywords": [
        "mstfkhazaal",
        "laravel",
        "filament-title-with-slug"
    ],
    "homepage": "https://github.com/mstfkhazaal/filament-title-with-slug",
    "support": {
        "issues": "https://github.com/mstfkhazaal/filament-title-with-slug/issues",
        "source": "https://github.com/mstfkhazaal/filament-title-with-slug"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "mstfkhazaal",
            "email": "mstfkhazaal@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/forms": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Mstfkhazaal\\FilamentTitleWithSlug\\": "src/",
            "Mstfkhazaal\\FilamentTitleWithSlug\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mstfkhazaal\\FilamentTitleWithSlug\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mstfkhazaal\\FilamentTitleWithSlug\\FilamentTitleWithSlugServiceProvider"
            ],
            "aliases": {
                "FilamentTitleWithSlug": "Mstfkhazaal\\FilamentTitleWithSlug\\Facades\\FilamentTitleWithSlug"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}